        tools:targetApi="31">
        <activity
            android:name=".MainAppMenu"
            android:exported="false"
            android:label="@string/title_activity_main_app_menu" />
        <activity
            android:name=".RegisterFinish"
            android:exported="false" />
        <activity
            android:name=".RegisterInput"
            android:exported="false" />
        <activity
            android:name=".SplashActivityOne"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
